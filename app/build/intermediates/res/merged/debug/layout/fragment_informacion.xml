<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="#ffffff">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:id="@+id/linearLayout5"
            android:padding="5dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center_horizontal">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#3f51b5"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_q_calidad"
                    android:padding="5dp" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/qcalidad" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/qcalidad_descripcion"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/bandera_azul"
                        android:gravity="right" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/bandera_azul_descripcion"
                        android:gravity="right"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imgBanderaAzul"
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#2196f3"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_bandera_azul"
                    android:padding="5dp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#e91e63"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_accesible"
                    android:padding="5dp"
                    android:id="@+id/imgAccesible" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/accesibilidad" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/accesibilidad_descripcion"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/duchas"
                        android:gravity="right" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/duchas_descripcion"
                        android:gravity="right"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#f44336"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_ducha"
                    android:padding="5dp"
                    android:id="@+id/imgDucha" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>




            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#9c27b0"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_hosteleria"
                    android:padding="5dp"
                    android:id="@+id/imgHosteleria" />

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/hosteleria" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/hosteleria_descripcion"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/parking"
                        android:gravity="right" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/parking_descripcion"
                        android:gravity="right"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#673ab7"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_parking"
                    android:padding="5dp"
                    android:id="@+id/imgParking" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#795548"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_pesca"
                    android:padding="5dp"
                    android:id="@+id/imgPesca" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/pesca" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/pesca_descripcion"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/salvamento"
                        android:gravity="right" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/salvamento_descripcion"
                        android:gravity="right"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>


                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#009688"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_salvamento"
                    android:padding="5dp"
                    android:id="@+id/imgSalvamento" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>





            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#4caf50"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_servicios"
                    android:padding="5dp"
                    android:id="@+id/imgServicios" />
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/servicios" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/servicios_descripcion"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="fill_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginRight="10dp">

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="@string/surf"
                        android:gravity="right" />

                    <TextView
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:textAppearance="?android:attr/textAppearanceSmall"
                        android:text="@string/surf_descripcion"
                        android:gravity="right"
                        android:textSize="12dp"
                        android:textColor="#9e9e9e" />
                </LinearLayout>


                <ImageView
                    android:layout_height="50dp"
                    android:layout_width="50dp"
                    android:visibility="visible"
                    android:background="#ff9800"
                    android:layout_marginRight="2dp"
                    android:src="@mipmap/ic_surf"
                    android:padding="5dp"
                    android:id="@+id/imgSurf" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#e0e0e0">

            </View>








        </LinearLayout>
    </ScrollView>


</LinearLayout>